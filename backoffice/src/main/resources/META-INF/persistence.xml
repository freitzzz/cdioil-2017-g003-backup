<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence">
  <persistence-unit name="cdioil-backoffice">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <properties>
      <!--            <property name="javax.persistence.jdbc.url" value="jdbc:h2:../db/demoapplication;MV_STORE=FALSE"/>-->
      <property name="javax.persistence.jdbc.url" value="jdbc:h2:..\db\demo-application;MV_STORE=FALSE"/>
      <property name="javax.persistence.jdbc.password" value=""/>
      <property name="javax.persistence.jdbc.driver" value="org.h2.Driver"/>
      <property name="javax.persistence.jdbc.user" value=""/>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
      <!--<property name="eclipselink.logging.level" value="FINE"/>-->
    </properties>
  </persistence-unit>
</persistence>
